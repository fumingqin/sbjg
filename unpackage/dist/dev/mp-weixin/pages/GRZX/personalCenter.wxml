<view><view class="topBox"><image class="backClass" src="../../static/GRZX/backImg.png"></image><view data-event-opts="{{[['tap',[['checkLogin',['$event']]]]]}}" class="portraitBox" bindtap="__e"><image class="portraitClass" src="{{portrait||'/static/GRZX/icon-headImg.png'}}"></image></view><text data-event-opts="{{[['tap',[['checkLogin',['$event']]]]]}}" class="nameClass" bindtap="__e">{{userName}}</text><view class="stationBox commonBox"><text class="numClass">{{typeNum}}<text class="fs">种</text></text><text class="fontClass">设备类型</text></view><view class="equipmentBox commonBox"><text class="numClass">{{equipmentNum}}<text class="fs">台</text></text><text class="fontClass">设备总数</text></view><view class="lineClass"></view></view><view class="centerBox"><block wx:for="{{modularList}}" wx:for-item="item" wx:for-index="index" wx:key="index"><view class="modularClass" hover-class="btnClass" data-event-opts="{{[['tap',[['operation',['$0'],[[['modularList','',index,'title']]]]]]]}}" bindtap="__e"><image class="{{[item.style]}}" src="{{item.icon}}"></image><text class="titleClass">{{item.title}}</text></view></block></view><view class="bottomBox"><block wx:for="{{functionList}}" wx:for-item="item" wx:for-index="index" wx:key="index"><view class="functionClass" hover-class="btnClass" data-event-opts="{{[['tap',[['operation',['$0'],[[['functionList','',index,'title']]]]]]]}}" bindtap="__e"><image class="{{[item.style]}}" src="{{item.icon}}"></image><text class="titleClass">{{item.title}}</text><image class="{{[item.rightClass]}}" src="../../static/GRZX/icon-right.png"></image></view></block></view></view>